<!doctype html>
<html>
  <%- include('shared/head') -%>
  <body>
    <%- include('shared/nav') -%>
    <div class='core-content'>
      <%- include('shared/header') -%>
      <div class='body-wrapper'>
        <div class='body-content'>
            <a href='https://developer.xero.com/documentation/api/contacts' target='_blank'>
              <h4>https://developer.xero.com/documentation/api/contacts</h4>
            </a>
            <p><strong>Read all Contacts - count:</strong> <%= contactsCount %></p>
            <p><strong>All Contacts:</strong>
              <ol>
              <% for(var i=0; i < allContacts.length; i++) { %>
                <li><%= allContacts[i].name %></li>
              <% } %>
              </ol>
        </div>
      </div>
    </div>
  </body>
</html>
